<div class="indent-left">
    {{#if active}}
      {{#each model as |account|}}
        <button {{action 'deleteAccount' account}}>Delete</button>
        Gebruiker: {{account.id}}<br><br>
      {{/each}}

      {{#each employees as |employee|}}
        RFID: {{employee.id}}<br>
        Active: {{employee.active}}<br>
        LED: <button {{action 'toggleLED'}}>Aan/Uit!</button>{{employee.led}}<br>
        Servo: <button {{action 'servoToOrigin'}}>Naar Startpositie!</button><br><br>
      {{/each}}
    {{else}}
      <h1>Houd je rfid erbij!</h1>
    {{/if}}
</div>
{{outlet}}
