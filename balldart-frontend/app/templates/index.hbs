<div class="container">
  <div class="inbl">
    {{#link-to "account" (query-params accountmode=" ")}}
      <img src="./assets/images/BlueSingleplayerCircle.png" alt="Singleplayer" class="imageSize">
      <div class="centered">Singleplayer</div>
    {{/link-to}}
  </div>
  <div class="inbl">
    {{#link-to "enter-players"}}
      <img src="./assets/images/RedMultiplayerCircle.png" alt="Multiplayer" class="imageSize">
      <div class="centered">Multiplayer</div>
    {{/link-to}}
  </div>
</div>



<div class="footer navbar-fixed-bottom">
  <div class="support-img">
    {{#link-to "support"}}
      <img src="./assets/images/questionmark.jpg" alt="Support" class="support">
    {{/link-to}}
  </div>
  {{#link-to "privacy-policy"}}
    <h5>Privacy Policy</h5>
  {{/link-to}}
</div>
{{outlet}}
